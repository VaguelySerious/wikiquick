{
  "name": "WikiQuick",
  "version": "1.0",
  "description": "An extension for conveniently looking up wikipedia info",
  "manifest_version": 2,
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    // "default_icon": "",
    "default_popup": "popup.html",
    "default_title": "WikiQuick"
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
}
