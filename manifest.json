{
  "name": "WikiQuick",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "An extension for conveniently looking up wikipedia info",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "web_accessible_resources": ["inject.js"],
  "browser_action": {
    "default_title": "JUST DO IT (manifest.json)"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
}
